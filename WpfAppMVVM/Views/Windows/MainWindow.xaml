<Window x:Class="WpfAppMVVM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:local="clr-namespace:WpfAppMVVM" 
        xmlns:views="clr-namespace:WpfAppMVVM.WPF.Views"
        xmlns:vm="clr-namespace:WpfAppMVVM.WPF.ViewModels"
        xmlns:behaviors="clr-namespace:WpfAppMVVM.WPF.Behaviors"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="450" Width="800" 
        DataContext="{vm:MainWindowViewModel}" AllowDrop="True">
    <!--<Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>-->
    <i:Interaction.Behaviors>
        <behaviors:DropData/>
    </i:Interaction.Behaviors>
    <Grid>
        <views:DictionaryView/>
    </Grid>
</Window>
